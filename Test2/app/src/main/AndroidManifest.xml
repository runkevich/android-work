        </activity>

        <activity android:name=".homework.hw2.Homework2Activity"
            android:parentActivityName=".homework.HomeworkMain">

        </activity>
        <activity android:name=".homework.hw1.Homework1Activity"
            android:parentActivityName=".homework.HomeworkMain">

        </activity>


        <activity android:name=".classwork.classwork2.Classwork2Activity"
            android:parentActivityName=".classwork.classwork1.Classwork1Activity">

        </activity>

